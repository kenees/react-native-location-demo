{
  "cli": {
    "version": ">= 4.1.1"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "channel": "development",
      "android": {
        "buildType": "apk",
        "credentialsSource": "local"
      },
      "ios": {
        "buildConfiguration": "Debug",
        "credentialsSource": "remote"
      }
    },
    "production": {
      "channel": "production",
      "android": {
        "buildType": "apk",
        "credentialsSource": "local"
      },
      "ios": {
        "buildConfiguration": "Release"
      }
    }
  }
}
